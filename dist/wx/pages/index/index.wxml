<cml-buildin-page title="chameleon" class=" cml-base cml-page">
  <cml-buildin-scroller height="{{-1}}" class=" cml-base cml-scroller">
    <view class="scroller-wrap  cml-base cml-view">
      <demo-com title="{{title}}" image-src="{{chameleonSrc}}" class=" cml-base cml-demo-com"></demo-com>
    </view>
    <my-component name="{{title}}" bindonshow="_cmlEventProxy" data-eventonshow="showHandle" class=" cml-base cml-my-component"></my-component>
  </cml-buildin-scroller>
</cml-buildin-page>